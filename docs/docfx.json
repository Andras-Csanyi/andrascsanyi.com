{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ]
      }
    ],
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Software Engineering",
      "_appTitle": "Software Engineering",
      "_enableSearch": true
    }
  }
}