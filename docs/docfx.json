{
  "metadata": [
    {
      "src": [
        {
          "files": ["DataStructuresAndAlgs/DataStructuresAndAlgs.csproj"],
          "src": "../langs/csharp/Core/"
        }
      ],
      "dest": "dsaapi",
      "outputFormat": "apiPage"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**", "obj/**"]
      }
    ],
    "resource": [
      {
        "files": ["**/images/**"]
      }
    ],
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appName": "Software Engineering",
      "_appTitle": "Software Engineering",
      "_enableSearch": true,
      "_disableContribution": true,
      "_appFooter": ""
    },
    "sitemap": {
      "baseUrl": "https://andrascsanyi.com",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "fileMetadata": {
      "_appTitle": {
        "blog/**/*.md": "blog",
        "software_engineering/**/*.md": "book"
      }
    }
  }
}
